{% extends '::base.html.twig' %}

{% block title %}{{ parent() }} Main Bundle{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets output='css/main.css' filter='cssrewrite' debug=false
    'bundles/colldevmain/css/layout.css' -%}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block content -%}{% endblock %}

{% block javascripts -%}
    {{ parent() }}
    {% javascripts output='js/main.js' debug=false
    '@CollDevMainBundle/Resources/public/js/jquery.js'
    '@CollDevMainBundle/Resources/public/js/scripts.js' -%}
    <script src="{{ asset_url }}"></script>
    {%- endjavascripts -%}
{% endblock %}